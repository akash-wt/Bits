generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  pubKey    String   @unique
  name      String?
  nonce     String?   
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  inboxes Inbox[]
}

model Inbox {
  id        String   @id @default(uuid())
  userId    String
  message   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  user User @relation(fields: [userId], references: [id])
}